{
  "name": "p2p-federated-learning-server",
  "version": "1.0.0",
  "description": "P2P Federated Learning Server with libp2p and TensorFlow.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "dev:node1": "cross-env P2P_PORT=9000 PORT=5000 NODE_ENV=development tsx src/index.ts",
    "dev:node2": "cross-env P2P_PORT=9001 PORT=5001 NODE_ENV=development tsx src/index.ts",
    "dev:node3": "cross-env P2P_PORT=9002 PORT=5002 NODE_ENV=development tsx src/index.ts",
    "dev:debug": "cross-env DEBUG=libp2p* NODE_ENV=development tsx src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "start:prod": "NODE_ENV=production PORT=3001 node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "test:local": "concurrently \"npm run dev:node1\" \"npm run dev:node2\"",
    "test:debug": "cross-env DEBUG=libp2p* npm run test:local"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^16.1.3",
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@libp2p/bootstrap": "^11.0.38",
    "@libp2p/identify": "^3.0.32",
    "@libp2p/interface": "^2.10.2",
    "@libp2p/kad-dht": "^15.1.2",
    "@libp2p/mdns": "^11.0.38",
    "@libp2p/peer-id-factory": "^4.2.4",
    "@libp2p/ping": "^2.0.32",
    "@libp2p/tcp": "^10.1.13",
    "@multiformats/multiaddr": "^12.4.0",
    "@tensorflow/tfjs": "^4.22.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "libp2p": "^2.8.8",
    "multer": "^2.0.1",
    "nanoid": "^5.1.5",
    "socket.io": "^4.8.1",
    "zod": "^3.25.32",
    "zod-validation-error": "^3.4.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "keywords": [
    "p2p",
    "federated-learning",
    "libp2p",
    "tensorflow",
    "machine-learning",
    "decentralized",
    "peer-to-peer"
  ],
  "author": "Hivemind",
  "license": "AGPL-version-3.0",
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.23",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.18.1",
    "eslint": "^9.27.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
